<plugin name='war-exec' version='1.0.1' grailsVersion='2.0 &gt; *'>
  <author>David Tinker</author>
  <authorEmail>david.tinker@gmail.com</authorEmail>
  <title>War Exec Plugin</title>
  <description>
Makes the war files generated by Grails executable (java -jar myapp.war) by embedding Jetty. Jetty can be configured
using properties in Config.groovy.
</description>
  <documentation>https://github.com/davidtinker/war-exec</documentation>
  <type>WarExecGrailsPlugin</type>
  <resources />
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <runtime>
      <dependency group='org.eclipse.jetty.aggregate' name='jetty-plus' version='8.1.4.v20120524' />
      <dependency group='org.eclipse.jetty.aggregate' name='jetty-webapp' version='8.1.4.v20120524' />
      <dependency group='org.eclipse.jetty.aggregate' name='jetty-websocket' version='8.1.4.v20120524' />
      <dependency group='org.eclipse.jetty.aggregate' name='jetty-servlet' version='8.1.4.v20120524' />
      <dependency group='org.eclipse.jetty.aggregate' name='jetty-server' version='8.1.4.v20120524' />
    </runtime>
  </dependencies>
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>